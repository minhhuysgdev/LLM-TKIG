        "    \n",
        "    print(\"\\\\nðŸ“ Files created:\")\n",
        "    for file_type, file_path in export_summary['files_created'].items():\n",
        "        print(f\"  {file_type}: {Path(file_path).name}\")\n",
        "    \n",
        "    return export_summary\n",
